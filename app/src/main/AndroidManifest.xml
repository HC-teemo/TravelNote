<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.nxnd.travelnote">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.nxnd.travelnote.activity.MainActivity">
        </activity>
        <activity android:name="com.nxnd.travelnote.activity.LoginActivity"/>
        <activity android:name="com.nxnd.travelnote.activity.RegisterActivity"/>
        <activity android:name="com.nxnd.travelnote.activity.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.nxnd.travelnote.activity.TravelNotesActivity"/>
        <activity android:name="com.nxnd.travelnote.activity.AboutMeActivity"/>
        <activity android:name="com.nxnd.travelnote.activity.LikeArticleActivity"/>
        <activity android:name="com.nxnd.travelnote.activity.FeedBackActivity"/>
        <activity android:name="com.nxnd.travelnote.activity.MeActivity"/>
        <activity android:name="com.nxnd.travelnote.activity.RaidersActivity"/>
    </application>

</manifest>